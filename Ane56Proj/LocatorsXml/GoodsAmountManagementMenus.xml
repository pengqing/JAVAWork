<?xml version="1.0" encoding="GBK"?>
<GoodsAmountManagementMenus>

<!-- 操作货量分配-西安分拨中心 -->	
	<operateGoodsAmountAssign>
		<!-- 货量分配按钮 -->
		<property name="goodsAmountAssignButton_xpath" value="/html/body/div[1]/div[2]/div/div[3]/div[2]/div/div/div/div[2]/div/div/div/div[2]/div[2]/div/div/div/div/div[1]/div[1]/button[2]"/>
		<!-- 交接单号输入框 -->
		<property name="equipmentReceiptNoInput_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/table[1]/tbody/tr[1]/td[4]/input"/>
		<!-- 装卸时间 -->
		<property name="handleTime_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/table[1]/tbody/tr[2]/td[8]/i/input"/>
		<!-- 所属队名1 -->
		<property name="belongsToTeamName1_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/div[1]/div[3]/table/tbody[2]/tr/td[1]/div/input"/>
		<!-- 所属组名1 -->
		<property name="belongsToGroupName1_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/div[1]/div[3]/table/tbody[2]/tr/td[2]/div/input"/>
