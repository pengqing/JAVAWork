		<!-- 操作人员 -->
		<property name="operatorName_xpath" value="/html/body/div[1]/div[2]/div/div[3]/div[2]/div/div/div/div[2]/div/div/div/div[2]/div[2]/div/div/div/div/div[2]/div[3]/table/tbody/tr/td/table/tbody/tr[3]/td/table/tbody/tr[3]/td[4]/div/input"/>
		<!-- 查询按钮 -->
		<property name="queryButton_xpath" value="/html/body/div[1]/div[2]/div/div[3]/div[2]/div/div/div/div[2]/div/div/div/div[2]/div[2]/div/div/div/div/div[1]/div[1]/button[1]"/>
		<!-- 查出的操作人工号 -->
		<property name="operatorJobNo_xpath" value="/html/body/div[1]/div[2]/div/div[3]/div[2]/div/div/div/div[2]/div/div/div/div[2]/div[2]/div/div/div/div/div[3]/div/div[2]/div[1]/div/div[1]/div[3]/table/tbody[2]/tr/td[5]/div"/>
