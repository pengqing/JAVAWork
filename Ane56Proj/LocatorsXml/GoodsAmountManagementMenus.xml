		<property name="operaterName1_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/div[1]/div[3]/table/tbody[2]/tr/td[3]/div/div/input"/>
		<!-- 分配重量1 -->
		<property name="assignWeight1_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/div[1]/div[3]/table/tbody[2]/tr/td[6]/div/input"/>
		<!-- 操作区域的新增按钮 -->
		<property name="addButton_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/div[1]/div[3]/table/tbody[2]/tr/td[7]/div/table/tbody/tr/td/table/tbody/tr/td[1]/button"/>
		<!-- 所属队名2 -->
		<property name="belongsToTeamName2_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/div[1]/div[3]/table/tbody[2]/tr[2]/td[1]/div/input"/>
		<!-- 所属组名2 -->
		<property name="belongsToGroupName2_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/div[1]/div[3]/table/tbody[2]/tr[2]/td[2]/div/input"/>
		<!-- 操作人姓名2 -->
		<property name="operaterName2_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/div[1]/div[3]/table/tbody[2]/tr[2]/td[3]/div/div/input"/>
		<!-- 分配重量2 -->
		<property name="assignWeight2_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/div[1]/div[3]/table/tbody[2]/tr[2]/td[6]/div/input"/>
