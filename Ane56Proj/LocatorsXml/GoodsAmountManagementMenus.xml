		<property name="loadWeight_xpath" value="/html/body/div[1]/div[2]/div/div[3]/div[2]/div/div/div/div[2]/div/div/div/div[2]/div[2]/div/div/div/div/div[3]/div/div[2]/div[1]/div/div[1]/div[3]/table/tbody[2]/tr/td[6]/div"/>
