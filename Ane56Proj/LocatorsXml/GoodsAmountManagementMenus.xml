		<property name="averageAssign_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/table[1]/tbody/tr[3]/td[7]/span/input"/>
		<!-- 分配确认按钮 -->
		<property name="assignConfirm_xpath" value="/html/body/div[*]/div[3]/div/div/div/div/table[2]/tbody/tr/td/table/tbody/tr/td[1]/button"/>
		<!-- 确认按钮 -->
		<property name="confirmButton_xpath" value="/html/body/div[*]/div[3]/div/div/div/table[2]/tbody/tr/td/table/tbody/tr/td[1]/button"/>
		<!-- 分配成功时的提示信息 -->
		<property name="promptInfo_xpath" value="//span[contains(text(),'成功')]"/>
	</operateGoodsAmountAssign>

<!-- 操作货量查询-西安分拨中心 -->	
